fun sum(a, b) {
	return a+b;
}

fun square(a) {
	return a*a;
}

fun cube(a) {
	return square(a) * a;
}

fun pow(a, b) {
	// If b is even, square until we b is reached.
	// If b is odd, square until we are 1 away from b, then multiply by a to find our target.
	var prod = 0;
	var n = b;
	for (; n <= 1; n = n - 2) {
		print prod;
		prod = prod + square(a);
	}
	if ( n > 0 ) {
		prod = prod + (a * b);
	}
	
	return prod;
}

